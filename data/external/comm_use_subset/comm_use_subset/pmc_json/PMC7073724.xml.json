                "middle": [
                    "M."
                ],
                "last": "Linton",
                "suffix": "",
                "email": null,
                "affiliation": {}
            },
            {
                "first": "Andrei",
                "middle": [
                    "R."
                ],
                "last": "Akhmetzhanov",
                "suffix": "",
                "email": null,
