    "metadata": {
        "title": "Examining the Links between Biodiversity and Human Health: An Interdisciplinary Research Initiative at the U.S. Environmental Protection Agency",
        "authors": [
            {
                "first": "Montira",
                "middle": [
                    "J."
                ],
                "last": "Pongsiri",
                "suffix": "",
                "email": "pongsiri.montira@epa.gov",
                "affiliation": {}
            },
            {
                "first": "Joe",
                "middle": [],
                "last": "Roman",
