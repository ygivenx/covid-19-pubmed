    "metadata": {
        "title": "Critical care medicine for emerging Middle East respiratory syndrome: Which point to be considered?",
        "authors": [
            {
                "first": "Viroj",
                "middle": [],
                "last": "Wiwanitkit",
                "suffix": "",
                "email": null,
